

{
	"manifest_version": 2,

	"name": "ko-crt",
	"description": "example",
	"version": "2.0",

	"browser_action": {
		"default_icon" : "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"nativeMessaging",
		"notifications",
		"tabs",
		"webNavigation",
		"webRequest",
		"clipboardWrite",
		"notifications",
		"http://*/*",
		"https://*/*",
		"<all_urls>",
		"https://HostName.com/",
		"*://*/*"
  ]
}